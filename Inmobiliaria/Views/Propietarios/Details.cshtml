@model Inmobiliaria.Models.Propietario

<h2>Detalle de Propietario</h2>

<p><strong>DNI:</strong> @Model.Dni</p>
<p><strong>Nombre:</strong> @Model.Nombre</p>
<p><strong>Apellido:</strong> @Model.Apellido</p>
<p><strong>Teléfono:</strong> @Model.Telefono</p>
<p><strong>Email:</strong> @Model.Email</p>
<p><strong>Dirección:</strong> @Model.Direccion</p>
<p><strong>Activo:</strong> @(Model.Activo ? "Sí" : "No")</p>

<a asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
<a asp-action="Index">Volver</a>
