@model Inmobiliaria.Models.Propietario

<h2>Nuevo Propietario</h2>

<form asp-action="Create" method="post">
    <div><label asp-for="Dni"></label><input asp-for="Dni" /></div>
    <div><label asp-for="Nombre"></label><input asp-for="Nombre" /></div>
    <div><label asp-for="Apellido"></label><input asp-for="Apellido" /></div>
    <div><label asp-for="Telefono"></label><input asp-for="Telefono" /></div>
    <div><label asp-for="Email"></label><input asp-for="Email" /></div>
    <div><label asp-for="Direccion"></label><input asp-for="Direccion" /></div>
    <div><label asp-for="Activo"></label><input asp-for="Activo" type="checkbox" /></div>
    <button type="submit">Guardar</button>
    <a asp-action="Index">Volver</a>
</form>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
